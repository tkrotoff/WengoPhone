	@echo 'creating testread.all_cpp.cpp ...'; \
	rm -f testread.all_cpp.files testread.all_cpp.final; \
	echo "#define KDE_USE_FINAL 1" >> testread.all_cpp.final; \
	for file in testread.cpp ; do \
	  echo "#include \"$$file\"" >> testread.all_cpp.files; \
	  test ! -f $(srcdir)/$$file || egrep '^#pragma +implementation' $(srcdir)/$$file >> testread.all_cpp.final; \
	done; \
	cat testread.all_cpp.final testread.all_cpp.files  > testread.all_cpp.cpp; \
	rm -f testread.all_cpp.final testread.all_cpp.files

